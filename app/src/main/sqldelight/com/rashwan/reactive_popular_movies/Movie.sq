CREATE TABLE movie (
    _id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    id INTEGER NOT NULL ,
    title TEXT,
    release_date TEXT,
    vote_average TEXT,
    overview TEXT,
    poster_path TEXT,
    backdrop_path TEXT,
    CONSTRAINT unique_id UNIQUE (id) ON CONFLICT REPLACE
);

select_all_movies:
SELECT *
FROM movie;

select_by_movie_id:
SELECT *
FROM movie
where id = ?;

